<?xml version="1.0"?>
<extension key="hidetimes" type="module">
  <file>hidetimes</file>
  <name>hidetimes</name>
  <description>A CiviCRM extension used to optionally hide times on the Event Info page with RegX</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Brienne Kordis</author>
    <email>brienne@megaphonetech.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/briennekordis/hideTimes</url>
    <url desc="Documentation">https://github.com/briennekordis/hideTimes/blob/main/README.md</url>
    <url desc="Support">https://github.com/briennekordis/hideTimes</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-07-07</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.63</ver>
  </compatibility>
  <comments>This is a custom CiviCRM extension, built and maintained by Megaphone Technology</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Hidetimes</namespace>
    <format>23.02.1</format>
    <angularModule>crmHidetimes</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
</extension>
